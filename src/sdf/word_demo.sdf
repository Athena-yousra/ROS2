    </axis>
   </joint>



<plugin
    filename="libignition-gazebo-diff-drive-system.so"
    name="ignition::gazebo::systems::DiffDrive">
    
    <!-- Left side wheels -->
    <left_joint>left_wheel_joint</left_joint>
    <left_joint>left_wheel_joint2</left_joint>

    <!-- Right side wheels -->
    <right_joint>right_wheel_joint</right_joint>
    <right_joint>right_wheel_joint2</right_joint>

    <!-- Parameters -->
    <wheel_separation>1.2</wheel_separation>
    <wheel_radius>0.4</wheel_radius>
    <odom_publish_frequency>20</odom_publish_frequency>
    <topic>cmd_vel</topic>
</plugin>

<!-- Moving Forward-->
<plugin filename="libignition-gazebo-triggered-publisher-system.so"
        name="ignition::gazebo::systems::TriggeredPublisher">
    <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
        <match field="data">87</match> <!-- W key -->
    </input>
    <output type="ignition.msgs.Twist" topic="/cmd_vel">
        linear: {x: 3}, angular: {z: 0.0}
    </output>
</plugin>

<!-- Moving Backward-->
<plugin filename="libignition-gazebo-triggered-publisher-system.so"
        name="ignition::gazebo::systems::TriggeredPublisher">
    <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
        <match field="data">83</match> <!-- S key -->
    </input>
    <output type="ignition.msgs.Twist" topic="/cmd_vel">
        linear: {x:-3}, angular: {z: 0.0}
    </output>
</plugin>

<!-- Turning Left-->
<plugin filename="libignition-gazebo-triggered-publisher-system.so"
        name="ignition::gazebo::systems::TriggeredPublisher">
    <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
        <match field="data">65</match> <!-- A key -->
    </input>
    <output type="ignition.msgs.Twist" topic="/cmd_vel">
        linear: {x: 3}, angular: {z: 0.5}
    </output>
</plugin>

<!-- Turning Right-->
<plugin filename="libignition-gazebo-triggered-publisher-system.so"
        name="ignition::gazebo::systems::TriggeredPublisher">
    <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
        <match field="data">68</match> <!-- D key -->
    </input>
    <output type="ignition.msgs.Twist" topic="/cmd_vel">
         linear: {x: 3}, angular: {z: -0.5}
    </output>
</plugin>


</model>
